            <stringProp name="BeanShellSampler.query">cnt = Integer.parseInt(vars.get(&quot;avail_courses_matchNr&quot;));

Random rand = new Random();

rand.setSeed(System.currentTimeMillis());

int n = rand.nextInt(cnt) + 1;

if (vars.get(&quot;selected_course_id&quot;) != null)
	vars.remove(&quot;selected_course_id&quot;);

vars.put(&quot;selected_course_id&quot;, vars.get(&quot;avail_courses_&quot; + n.toString()));

</stringProp>
