<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <connectionStrings>
    <add name="NpgsqlConnection"
			connectionString="Server=urs_db;Database=postgres;User ID=postgres;Password=secret;" 
			providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="AllowHttp" value="true" />
    <add key="IP" value="*" />
    <add key="Port" value="8090" />
    <add key="FacebookAppId" value="XXXXX" />
    <add key="FacebookAppSecret" value="XXXXX" />
    <add key="TokenExpiryInDays" value="14" />
    <add key="SqlFileLocations" value="Sql,sql,SQL"/>
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>